@apiUrl = http://localhost:8000
@token = {{login.response.body.body.token}}
### VERSION AND CHECK SERVER CONNECTION
GET {{apiUrl}}/version
Content-Type: application/json


#### GET ALL STATES
POST {{apiUrl}}/master/states
Content-Type: application/json
Authorization: Bearer {{token}}

{
    "fields" : ["states.id","states.name"],
    "max" : 102,
    "current" : 0,
    "relation" : null
}

#### GET ALL STATE BY ID
POST {{apiUrl}}/master/states/byId
Content-Type: application/json
Authorization: Bearer {{token}}

{
    "fields" : ["states.id","states.name"],
    "state_id": 36,
    "relation": null
}

#### ADD NEW STATE
POST {{apiUrl}}/master/states/new
Content-Type: application/json
Authorization: Bearer {{token}}

{
    "state_name": "ZSD"
}


#### UPDATE STATES
POST {{apiUrl}}/master/states/update
Content-Type: application/json
Authorization: Bearer {{token}}

{
    "updates": {
        "states.name": "WP"
    },
    "conditions":{
        "states.id": 1
    }
}

#### DELETE STATE
POST {{apiUrl}}/master/states/delete
Content-Type: application/json
Authorization: Bearer {{token}}

{
    "state_id": 35
}

